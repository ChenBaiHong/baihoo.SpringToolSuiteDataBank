package com.baihoo.springboot.common.criteria;

import java.util.List;

import com.baihoo.springboot.common.criteria.type.ExprType;
import com.baihoo.springboot.common.criteria.type.LogicType;
import com.baihoo.springboot.common.criteria.type.OrderByType;
import com.baihoo.springboot.common.criteria.type.OrderType;

/**
 * baiHoo 查询条件组织
 * @author Administrator
 *
 */
public class CriteriaType {
	/**
	 * @param _distinct 是否去重
	 * @param _entity 实体
	 * @param _expr	操作字段
	 * @param _and    
	 * @param order
	 * @param orderBy 
	 */
	Boolean _distinct;
	String _entity;
	List<ExprType> _expr;
	List<LogicType> _and;
	List<OrderType> order;
	List<OrderByType> orderBy;
}
